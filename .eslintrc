{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": false,
    "node": true
  },
  "rules": {
    "arrow-parens": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "no-undef": "off",
    "operator-linebreak": "off",
    "no-debugger": "warn",
    "semi": "off",
    "no-invalid-this": "off",
    "max-len": "off",
    "no-unused-vars": "warn",
    "no-case-declarations": "off"
  },
  "extends": ["eslint:recommended", "google"]
}
